{
  "compilerOptions": {
    "target": "ES6",
    "module": "ES6",
    "moduleResolution": "node",
    "lib": ["ES6"],
    "outDir": "../client",
    "rootDir": ".",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["models/**/*", "apis/**/*", "index.ts"],
  "exclude": ["node_modules"]
}
